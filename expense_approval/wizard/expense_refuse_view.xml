<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_expense_refuse_reason_custom" model="ir.ui.view">
        <field name="name">view.expense.refuse.reason.custom</field>
        <field name="model">hr.expense.refuse.wizard</field>
        <field name="inherit_id" ref="hr_expense.hr_expense_refuse_wizard_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='action_refuse']" position="replace">
                <button string="Refuse" name="action_reject_custom" type="object" class="oe_highlight" data-hotkey="q"/>
            </xpath>
        </field>
    </record>
</odoo>
